  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.0",
    "@babel/runtime": "^7.18.3",
    "@ctrl/tinycolor": "^3.6.0",
    "classnames": "^2.3.2",
    "rc-motion": "^2.7.3",
    "rc-util": "^5.33.0"
  },
  "devDependencies": {
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/classnames": "^2.2.10",
